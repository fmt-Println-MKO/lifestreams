<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	
	
   	<bean id="requesteeSet" class="org.springframework.util.StringUtils" factory-method="commaDelimitedListToSet">
       	<constructor-arg type="java.lang.String" value="${ohmage.requestees}"/>
   	</bean>

	

	
	<bean id= "requester" class="org.ohmage.models.OhmageUser">
		<constructor-arg value="${ohmage.server}"/>
		<constructor-arg value="${lifestreams.username}"/>
		<constructor-arg value="${lifestreams.password}"/>
	</bean>

</beans>
